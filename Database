CREATE TABLE [dbo].[Accounts] (
    [AccountID]     INT         IDENTITY (1, 1) NOT NULL,
	  [Account Number]INT		    NOT NULL,
    [Account Name ] NCHAR (100) NOT NULL,
    [Street]        NCHAR (100) NOT NULL,
    [City]          NCHAR (100) NOT NULL,
    [Zipcode]       INT         NOT NULL,
    [State]         NCHAR (100) NOT NULL,
    [Amount Due]    FLOAT (53)  NOT NULL,
    [Total Paid]    FLOAT (53)  NOT NULL,
    [Amount Owed]   FLOAT (53)  NOT NULL,
    PRIMARY KEY CLUSTERED ([AccountID] ASC)
);


